with "util.gpr";
with "ini_file_manager.gpr";

project Default is

   for Main use ("synth.adb");
   for Source_Dirs use (".", "src");
   for Object_Dir use "build";

   package Pretty_Printer is
      for Default_Switches ("ada") use ("-c0", "-pU");
   end Pretty_Printer;

   package Compiler is
      for Default_Switches ("ada") use ("-g", "-O2", "-gnat12", "-gnatyaAbBcdefhiklnOprsSxt");
   end Compiler;

end Default;

